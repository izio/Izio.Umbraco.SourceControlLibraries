<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Izio.Umbraco.SourceControlLibraries</id>
        <version>1.1.0</version>
        <authors>Patrick Robin</authors>
        <owners>Patrick Robin</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Adds an ApplicationEventHandler that hooks in to the ApplicationStarted event that copies the dll's associated with any Umbraco packages that have been installed to a separate folder so that they can be added to source control.  The dll's that have been added to source control will then be copied back to the bin folder if they are missing.</description>
        <releaseNotes>Fixed file extension</releaseNotes>
        <copyright>Copyright 2016</copyright>
        <tags>Umbraco Development</tags>
        <dependencies>
            <dependency id="UmbracoCms.Core" version="7.5.3" />
        </dependencies>
    </metadata>
    <files>
        <file src="..\Izio.Umbraco.SourceControlLibraries\Config\SourceControlLibraries.config" target="content\Config\SourceControlLibraries.config" />
        <file src="..\Izio.Umbraco.SourceControlLibraries\bin\Debug\Izio.Umbraco.SourceControlLibraries.dll" target="lib\Izio.Umbraco.SourceControlLibraries.dll" />
        <file src="readme.txt" target="readme.txt" />
    </files>
</package>